# Discord Bot Token
DISCORD_TOKEN=YOUR_BOT_TOKEN_HERE

# Discord Application Client ID
CLIENT_ID=YOUR_CLIENT_ID_HERE
# Alias for clarity (some setups prefer DISCORD_* naming)
DISCORD_CLIENT_ID=YOUR_CLIENT_ID_HERE

# Discord Server (Guild) ID for development
GUILD_ID=YOUR_GUILD_ID_HERE
# Alias for clarity (some setups prefer DISCORD_* naming)
DISCORD_GUILD_ID=YOUR_GUILD_ID_HERE

# Slash command deployment scope: "guild" | "global"
# Default is "guild" when not set.
COMMANDS_SCOPE=guild

# Notion API Integration
NOTION_API_KEY=YOUR_NOTION_API_KEY_HERE
# The following Notion Database ID is a fallback.
# For multi-guild setups, it's recommended to set the ID per-server using the /config command.
NOTION_DATABASE_ID=YOUR_NOTION_DATABASE_ID_HERE
# Column/Property names in your Notion "Submissions" database.
# These must match the names in Notion exactly.
NOTION_NAME_COLUMN="Name"
NOTION_OWNER_COLUMN="Owner"
NOTION_DUEDATE_COLUMN="Due Date"
NOTION_STATUS_COLUMN="Status"
# Default value for the 'Status' column when a new item is added.
NOTION_STATUS_PENDING="Pending"

# Weather API (e.g., WeatherAPI.com)
WEATHER_API_KEY=YOUR_WEATHER_API_KEY_HERE

# Google Calendar API
GOOGLE_CALENDAR_ID=YOUR_GOOGLE_CALENDAR_ID_HERE
# Path to your Google service account credentials JSON file
GOOGLE_APPLICATION_CREDENTIALS=./path/to/your/credentials.json

# Google Gemini API
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE

# Google Sheets for Finance
GOOGLE_SHEET_ID=YOUR_GOOGLE_SHEET_ID_HERE
EXPENSE_REPORT_FORM_URL=https://docs.google.com/forms/d/your_form_id/viewform

# Supabase/PostgreSQL Database URL
DATABASE_URL="postgresql://USER:PASSWORD@HOST:PORT/DATABASE"

# Festival Countdown
FESTIVAL_START_DATE=2025-10-25

# Automatic Reminders
REMINDER_CHANNEL_ID=YOUR_DISCORD_CHANNEL_ID_HERE


